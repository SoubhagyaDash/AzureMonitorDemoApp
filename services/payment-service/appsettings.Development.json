{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "ConnectionStrings": {
    "EventHub": "UseDevelopmentStorage=true",
    "Redis": "localhost:6379"
  },
  "FailureInjection": {
    "Enabled": true,
    "LatencyMs": 500,
    "ErrorRate": 0.05,
    "TimeoutRate": 0.01
  }
}